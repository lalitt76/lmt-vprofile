>kops create cluster  --name=kops.tyagienterprises.com --state=s3://vprofile-kops-state-lmt \
--zones=us-east-1a,us-east-1b --node-count=2 --node-size=t3.small \
--master-size=t3.medium --dns-zone=kops.tyagienterprises.com \
--node-volume-size=8 --master-volume-size=8


>kops update cluster --name kops.tyagienterprises.com --state=s3://vprofile-kops-state-lmt --yes --admin